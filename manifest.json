{
    "manifest_version": 3,
    "name": "work manager by junhee",
    "description": "made by lee junhee",
    "version": "1.0",
    "icons": {
      "16": "images/16_sam.png",
      "32": "images/32_sam.png",
      "48": "images/48_sam.png",
      "128": "images/128_sam.png"
    },
    "action": {
      "default_popup": "popup.html"
    },

    "host_permissions": ["https://*/*"],

    "permissions": [
      "storage","activeTab","scripting"
    ]
  }